{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-splm-table.directive",
        "js/aw-panel-section.directive",
        "js/aw-chart.directive",
        "js/aw-row.directive",
        "js/aw-column.directive",
        "js/aw-i18n.directive",
        "js/aw-command-bar.directive",
        "js/aw-am-table-panel.directive"
    ],
    "props": {
        "sub-panel-context": {
            "type": "object"
        }
    },
    "lifecycleHooks": {
        "onUnmount": "removePieChartData",
        "onUpdate": [ {
            "action": "createPieChartForDynamicTables",
            "observers": [ "props.subPanelContext.context.vrSublocationState.pieChartData.vrTables.configTableTwoProvider.chartData" ]
        },
        {

            "action": "redrawConfigTableTwoPieChart",
            "observers": [ "props.subPanelContext.context.vrSublocationState.pieChartData.vrTables.configTableTwoProvider.redrawConfigTableTwoPieAsDoubleFilter" ]
        },
        {
            "action": "handleSelectionChange",
            "observers": [ "viewModel.atomicData.selectionData" ]
        } ]

    },
    "actions": {
        "handleSelectionChange": {
            "actionType": "JSFunction",
            "method": "handleSelectionChange",
            "inputData": {
                "data": "{{data.dataProviders.configTableTwoProvider}}",
                "subPanelContext": "{{subPanelContext}}"
            },
            "outputData": {},
            "deps": "js/Crt1VROverviewTablesService"
        },
        "redrawConfigTableTwoPieChart": {
            "actionType": "JSFunction",
            "method": "redrawSecondaryPieCharts",
            "inputData": {
                "input": "{{data}}"
            },
            "outputData": {
                "pieChartForConfigTableTwoProvider_chartPoints": ""
            },
            "deps": "js/Crt1VROverviewTablesService",
            "events": {
                "success": [ {
                    "name": "redrawPieChartForDynamicTables"
                } ]
            }
        },
        "removePieChartData": {
            "actionType": "JSFunction",
            "method": "removePieChartData",
            "inputData": {
                "vrSublocationState": "{{subPanelContext.context.vrSublocationState}}",
                "table": "configTableTwoProvider",
                "pieClickedFlag": "configTableTwoPieClicked",
                "pieDedrawFlag": "redrawConfigTableTwoPieAsDoubleFilter",
                "subPanelContext": "{{subPanelContext}}",
                "dataProvider": "{{data.dataProviders.configTableTwoProvider}}"
            },
            "deps": "js/Crt1VROverviewTablesService"
        },
        "editEventAction": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "plTable.editStateChange",
                        "eventData": {
                            "state": "{{data.eventData.state}}",
                            "dataSource": {
                                "dataProviders": "{{data.dataProviders}}"
                            }
                        }
                    }
                ]
            }
        },
        "handleVMCollectionUpdate": {
            "actionType": "JSFunction",
            "method": "handleHtmlPanelVMCollection",
            "deps": "js/xrtUtilities",
            "inputData": {
                "callback": "{{subPanelContext.callback.updateVMCollectionCallback}}",
                "response": {
                    "dataProvider": "{{data.dataProviders.configTableTwoProvider}}"
                }
            }
        },
        "doArrangeEvent": {
            "actionType": "JSFunctionAsync",
            "method": "arrangeColumns",
            "inputData": {
                "declViewModel": "{{data}}",
                "eventData": "{{data.eventData}}"
            },
            "deps": "js/dynamicTableUtils"
        },
        "saveColumnConfigLoadData": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-AWS2-2023-06-Finder",
            "method": "performSearchViewModel5",

            "inputData": {
                "columnConfigInput": {
                    "clientName": "AWClient",
                    "operationType": "as_arranged",
                    "clientScopeURI": "{{subPanelContext.context.vrSublocationState.scopeURI.get('ConfigTableTwo')}}"
                },
                "saveColumnConfigData": {
                    "clientScopeURI": "{{subPanelContext.context.vrSublocationState.scopeURI.get('ConfigTableTwo')}}",
                    "columnConfigId": "{{subPanelContext.context.vrSublocationState.columnConfig.get('ConfigTableTwo')}}",
                    "columns": "{{data.dataProviders.configTableTwoProvider.newColumns}}",
                    "scope": "LoginUser",
                    "scopeName": ""
                },
                "searchInput": {
                    "maxToLoad": 30,
                    "maxToReturn": 30,
                    "providerName": "Crt1AnalysisRequestInProvider",
                    "attributesToInflate":["crt1SourceObject"],
                    "searchCriteria": {
                        "parentUid": "{{function:getParentUid}}",
                        "dcpSortByDataProvider": "true",
                        "tableUID":"ConfigTableTwo"
                    },
                    "searchFilterFieldSortType": "Alphabetical",
                    "searchSortCriteria": "{{data.columnProviders.configTableTwoColumnProvider.sortCriteria}}",
                    "startIndex": "{{data.dataProviders.configTableTwoProvider.startIndex}}",
                    "columnFilters": "{{function:getColumnFiltersForDynamicTables}}"
                },
                "inflateProperties": true
            },
            "outputData": {
                "totalFound": "totalFound",
                "searchResults": "{{json:searchResultsJSON}}",
                "dataProviders.configTableTwoProvider.columnConfig": "{{function:getColumnDataForDynamicTables}}"
            },
            "deps": "js/Crt1VROverviewTablesService"
        },
        "resetColumnConfig": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-AWS2-2023-06-UiConfig",
            "method": "getOrResetUIColumnConfigs4",
            "inputData": {
                "getOrResetUiConfigsIn": [
                    {
                        "scope": "LoginUser",
                        "scopeName": "",
                        "clientName": "AWClient",
                        "resetColumnConfig": true,
                        "columnConfigQueryInfos": [
                            {
                                "clientScopeURI": "{{subPanelContext.context.vrSublocationState.scopeURI.get('ConfigTableTwo')}}",
                                "operationType": "configured",
                                "typeNames": [
                                    "WorkspaceObject"
                                ],
                                "columnsToExclude": []
                            }
                        ],
                        "businessObjects": [
                            {}
                        ]
                    }
                ]
            },
            "outputData": {
                "dataProviders.configTableTwoProvider.resetColumnConfigs": "{{function:getColumnDataForDynamicTables}}"
            },
            "events": {
                "success": [ {
                    "name": "configTableTwo.plTable.reload"
                } ]
            },
            "deps": "js/Crt1VROverviewTablesService"
        },
        "loadDataForConfigTableTwo": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-AWS2-2023-06-Finder",
            "method": "performSearchViewModel5",
            "inputData": {
                "columnConfigInput": {
                    "clientName": "AWClient",
                    "clientScopeURI": "{{subPanelContext.context.vrSublocationState.scopeURI.get('ConfigTableTwo')}}",
                    "operationType": "as_configured"
                },
                "searchInput": {
                    "maxToLoad": 30,
                    "maxToReturn": 30,
                    "providerName": "Crt1AnalysisRequestInProvider",
                    "attributesToInflate":["crt1SourceObject"],
                    "searchCriteria": {
                        "parentUid": "{{function:getParentUid}}",
                        "dcpSortByDataProvider": "true",
                        "tableUID":"ConfigTableTwo"
                    },
                    "searchFilterFieldSortType": "Alphabetical",
                    "searchSortCriteria": "{{data.columnProviders.configTableTwoColumnProvider.sortCriteria}}",
                    "startIndex": "{{data.dataProviders.configTableTwoProvider.startIndex}}",
                    "columnFilters": "{{function:getColumnFiltersForDynamicTables}}"
                },
                "inflateProperties": true
            },
            "outputData": {
                "totalFound": "totalFound",
                "searchResults": "{{json:searchResultsJSON}}",
                "dataProviders.configTableTwoProvider.columnConfig": "{{function:getColumnDataForDynamicTables}}"
            },
            "deps": "js/Crt1VROverviewTablesService"
        },
        "createPieChartForDynamicTables": {
            "actionType": "JSFunction",
            "method": "createPieChartForDynamicTables",
            "inputData": {
                "input": "{{data}}",
                "tableColumnFilters": "configTableTwoColumnFilters",
                "redrawFlag": "redrawConfigTableTwoPieAsDoubleFilter",
                "chartData": "{{subPanelContext.context.vrSublocationState.pieChartData.vrTables.configTableTwoProvider}}",
                "seriesName": "{{data.i18n.ConfigTable2}}",
                "subPanelContext": "{{subPanelContext.context.vrSublocationState}}",
                "table": "configTableTwoProvider"
            },
            "outputData": {
                "pieChartForConfigTableTwoProvider_chartPoints": ""
            },
            "deps": "js/Crt1PieChartsService"
        },
        "checkAndReloadConfigTableChart": {
            "actionType": "JSFunction",
            "method": "checkAndReloadConfigTableChart",
            "inputData": {
                "data": "{{data}}",
                "subPanelContext": "{{subPanelContext.context.vrSublocationState}}"
            },
            "deps": "js/Crt1ConfigTableService",
            "events": {
                "success": [ {
                    "name": "configTableTwoProvider.modelObjectsEdited"
                } ]
            }
        },
        "filterConfigTable": {
            "actionType": "JSFunction",
            "method": "filterConfigTable",
            "inputData": {
                "data": "{{data}}",
                "chartProp": "{{subPanelContext.context.vrSublocationState}}",
                "provider": "configTableTwoProvider",
                "pieClicked": "configTableTwoPieClicked",
                "configTableColumnFilter": "configTableTwoColumnFilters",
                "seriesName": "{{data.i18n.ConfigTable2}}"

            },
            "deps": "js/Crt1ConfigTableService",
            "events": {
                "success": [ {
                    "name": "configTableTwo.plTable.reload"
                } ]
            }
        },
        "displayAllConfigTable": {
            "actionType": "JSFunction",
            "method": "displayAllConfigTable",
            "inputData": {
                "data": "{{data}}",
                "chartProp": "{{subPanelContext.context.vrSublocationState}}",
                "provider": "configTableTwoProvider",
                "pieClicked": "configTableTwoPieClicked",
                "configTableColumnFilter": "configTableTwoColumnFilters",
                "seriesName": "{{data.i18n.ConfigTable2}}"
            },
            "deps": "js/Crt1ConfigTableService",
            "events": {
                "success": [ {
                    "name": "configTableTwo.plTable.reload"
                } ]
            }
        },
        "modelObjectsLoaded": {
            "actionType": "JSFunction",
            "method": "modelObjectsLoaded",
            "inputData": {
                "dataProviders": "{{data.dataProviders.configTableTwoProvider}}",
                "eventMap": "{{data.eventMap}}",
                "scopeSel": "{{props.subPanelContext.context.vrSublocationState.mselected[0]}}",
                "subPanleCtx": "{{props.subPanelContext}}"
            },
            "deps": "js/Crt1VROverviewTablesService"
        },
        "modelObjectEdited": {
            "actionType": "JSFunction",
            "method": "modelObjectEdited",
            "inputData": {
                "dataProvider": "{{data.dataProviders.configTableTwoProvider}}",
                "scopeSel": "{{props.subPanelContext.context.vrSublocationState.mselected[0]}}",
                "subPanleCtx": "{{props.subPanelContext}}"
            },
            "deps": "js/Crt1VROverviewTablesService"
        }
    },
    "functions": {
        "getColumnFiltersForDynamicTables": {
            "functionName": "getColumnFiltersForDynamicTables",
            "parameters": [
                "{{data.columnProviders.configTableTwoColumnProvider}}",
                "{{subPanelContext.context.vrSublocationState.pieChartData.vrTables.configTableTwoProvider.configTableColumnFilter}}",
                "{{subPanelContext.context.vrSublocationState.pieChartData.vrTables.topPieChart.testResultColumnFilters}}"
            ]
        },
        "getColumnDataForDynamicTables": {
            "functionName": "getColumnDataForDynamicTables",
            "parameters": [
                "{{data}}"
            ]
        },
        "getParentUid": {
            "functionName": "getParentUid",
            "parameters": [
                "{{subPanelContext}}"
            ]
        }
    },
    "columnProviders": {
        "configTableTwoColumnProvider": {
            "saveColumnAndLoadAction": "saveColumnConfigLoadData",
            "resetColumnAction": "resetColumnConfig",
            "sortCriteria": []
        }
    },
    "dataProviders": {
        "configTableTwoProvider": {
            "action": "loadDataForConfigTableTwo",
            "response": "{{data.searchResults}}",
            "totalFound": "{{data.totalFound}}",
            "selectionModelMode": "multiple",
            "editContext": "NONE",
            "commandsAnchor": "com.siemens.splm.clientfx.tcui.commands.modelObjectCellListActionCommands",
            "contextMenuCommandsAnchor": "aw_configTableTwo_contextMenu",
            "inputData": {
                "selectionData": "{{fields.selectionData}}"
            }
        }
    },
    "grids": {
        "configTableTwo": {
            "columnProvider": "configTableTwoColumnProvider",
            "dataProvider": "configTableTwoProvider",
            "enableArrangeMenu": true,
            "gridOptions": {
                "enableSorting": true,
                "maxRowsToShow": 10,
                "isFilteringEnabled": true
            }
        }
    },
    "data": {
        "chartProviders" : {
            "pieChartForConfigTableTwoProvider": {
                "chartType": "pie",
                "chartColorOverrideClass":"aw-verificationManagement-chartStatusColor",
                "chartConfig": {
                    "isChartZoomable": true,
                    "isYAxisLinearOrLogarithmic": "logarithmic",
                    "xAxisLabel": "",
                    "yAxisLabel": "",
                    "isDataLabelOnChartEnabled": true
                }
            }
        },
        "primaryActiveTabId": {
            "dbValue": "",
            "type": "OBJECT"
        },
        "selectionData": {
            "initialValues": {},
            "meta": {}
        }
    },
    "onEvent": [
        {
            "eventId": "columnArrange",
            "cacheEventData": true,
            "action": "doArrangeEvent"
        },
        {
            "eventId": "configTableTwoProvider.modelObjectsUpdated",
            "cacheEventData": true,
            "action": "modelObjectsLoaded"
        },
        {
            "eventId": "configTableTwoProvider.modelObjectsEdited",
            "cacheEventData": true,
            "action": "modelObjectEdited"
        },
        {
            "eventId": "configTableTwoProvider.modelObjectsUpdated",
            "cacheEventData": true,
            "action": "handleVMCollectionUpdate"
        },
        {
            "eventId": "editHandlerStateChange",
            "cacheEventData": true,
            "action": "editEventAction"
        },
        {
            "eventId": "plTable.editStateChange",
            "action": "checkAndReloadConfigTableChart"
        },
        {
            "eventId": "undefined.selected",
            "cacheEventData": true,
            "action": "filterConfigTable"
        },
        {
            "eventId": "undefined.unselected",
            "cacheEventData": true,
            "action": "displayAllConfigTable"
        },
        {
            "eventId": "Crt1ConfigTable.drawPieChart",
            "action": "loadDataForConfigTableTwoAfterEdit"
        },
        {
            "eventId": "redrawPieChartForDynamicTables",
            "action": "createPieChartForDynamicTables"
        },
        {
            "eventId": "resetPieChart",
            "action": "redrawConfigTableTwoPieChart"
        }

    ],
    "conditions": {
        "showConfigTableTwoMessageString": {
            "expression": "data.pieChartForConfigTableTwoProvider_chartPoints[0].keyValueDataForChart[0].value === 0 && data.pieChartForConfigTableTwoProvider_chartPoints[0].keyValueDataForChart[1].value === 0 && data.pieChartForConfigTableTwoProvider_chartPoints[0].keyValueDataForChart[2].value === 0 && data.pieChartForConfigTableTwoProvider_chartPoints[0].keyValueDataForChart[3].value === 0 && data.pieChartForConfigTableTwoProvider_chartPoints[0].keyValueDataForChart[4].value === 0"
        },
        "showConfigTableTwoChart" : {
            "expression": "data.pieChartForConfigTableTwoProvider_chartPoints && data.pieChartForConfigTableTwoProvider_chartPoints[0] && data.pieChartForConfigTableTwoProvider_chartPoints[0].keyValueDataForChart[0].value != -1"
        }
    },
    "i18n": {
        "NoResult": [
            "AnalysisRequestCommandPanelsMessages"
        ],
        "Fail": [
            "AnalysisRequestCommandPanelsMessages"
        ],
        "Pass": [
            "AnalysisRequestCommandPanelsMessages"
        ],
        "ConfigTable2":[
            "AnalysisRequestCommandPanelsMessages"
        ],
        "noDataToDisplayChart":[
            "AnalysisRequestCommandPanelsMessages"
        ],
        "Caution": [
            "AnalysisRequestCommandPanelsMessages"
        ],
        "Blocked": [
            "AnalysisRequestCommandPanelsMessages"
        ]
    }
}
