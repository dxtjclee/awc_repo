                {
                    "propDisplayValue": "{{i18n.resolved}}",
                    "propInternalValue": "resolved"
                },
                {
                    "propDisplayValue": "{{i18n.rejected}}",
                    "propInternalValue": "rejected"
                },
                {
                    "propDisplayValue": "{{i18n.reopened}}",
                    "propInternalValue": "reopened"
                }
            ]
        },
        "showOnPage":
        {
            "displayName": "{{i18n.textShown}}",
            "type": "STRING",
            "hasLov": "true",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue": "none"
        },
        "showOnPageValues":
        {
            "type": "STRING",
