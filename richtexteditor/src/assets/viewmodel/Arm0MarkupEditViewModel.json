        {
            "messageType": "WARNING",
            "messageText": "{{i18n.confirmOfficial}}",
            "navigationOptions":
            [
                {
                    "option": "Cancel",
                    "text": "{{i18n.cancel}}",
                    "action": "cancelOfficial"
                },
                {
                    "option": "Set",
                    "text": "{{i18n.set}}"
                }
            ]
        }
    },

    "i18n":
