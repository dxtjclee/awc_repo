                    {
                        "name": "tracebilityMatrix.getDetails",
                        "condition": "data.isReviseOps && data.isReviseOps === true"
                    },
                    {
                        "name": "requirementDocumentation.processDataFromServer"
                    }
                ],
                "failure": [
                    {
                        "name": "requirementDocumentation.processDataFromServer"
                    }
                ]
