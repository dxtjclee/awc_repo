            "inputData": {
                "path": "objectQuotaContext.useObjectQuota",
                "value": "true"
            }
        },
        "destroyTraceabilityMatrixContenxt": {
            "actionType": "batchJob",
            "steps": [ {
                    "action": "unregisterObjectQuotaContext"
                },
                {
                    "action": "unRegisterTraceabilityMatrixCellSelectedCtx"
                },
                {
                    "action": "destroyTraceabilityMatrix"
                }
            ]
        },
        "unregisterObjectQuotaContext": {
            "actionType": "JSFunction",
            "method": "unRegisterCtx",
            "deps": "js/appCtxService",
            "inputData": {
                "path": "objectQuotaContext"
            }
        },
        "actionOnMount": {
            "actionType": "batchJob",
            "steps": [ {
                    "action": "revealMatrix"
                },
