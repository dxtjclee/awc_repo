            },
            "useApplicationCond": {
                "expression": "ctx.preferences.PLE_Parameter_Create_With_Application_Ux && ctx.preferences.PLE_Parameter_Create_With_Application_Ux[0] === 'true'"
            },
            "isTcReleaseAtLeast142": {
                "expression": "ctx.tcSessionData.tcMajorVersion > 14 || (ctx.tcSessionData.tcMajorVersion === 14 && ctx.tcSessionData.tcMinorVersion >= 2)"
            },
            "selectedTypeCond": {
                "expression": "(xrtState.xrtTypeLoaded.type === 'Att0MeasurableAttributePnt' && !conditions.isNullPointX && !conditions.isNullPointY && !conditions.isNullPointZ) || xrtState.xrtTypeLoaded.type === 'Att0MeasurableAttributeDbl' || xrtState.xrtTypeLoaded.type === 'Att0MeasurableAttributeStr' || xrtState.xrtTypeLoaded.type === 'Att0MeasurableAttributeInt' || xrtState.xrtTypeLoaded.type === 'Att0MeasurableAttributeBool' || xrtState.xrtTypeLoaded.type === 'Wnb0MassMeasurableAttribute'"
            },
            "isNullPointX": {
                "expression": "xrtState.xrtVMO.props.att0PointX.dbValue === null || xrtState.xrtVMO.props.att0PointX.dbValue === undefined || xrtState.xrtVMO.props.att0PointX.dbValue === ''"
            },
            "isNullPointY": {
                "expression": "xrtState.xrtVMO.props.att0PointY.dbValue === null || xrtState.xrtVMO.props.att0PointY.dbValue === undefined || xrtState.xrtVMO.props.att0PointY.dbValue === ''"
            },
            "isNullPointZ": {
                "expression": "xrtState.xrtVMO.props.att0PointZ.dbValue === null || xrtState.xrtVMO.props.att0PointZ.dbValue === undefined || xrtState.xrtVMO.props.att0PointZ.dbValue === ''"
            },
            "isDefintionSelected":{
                "expression":"data.isDefinitionSelected === true"
            },
            "isInVRContext": {
                "expression": "(subPanelContext.openedObject.modelType.typeHierarchyArray.indexOf( 'Crt0VldnContractRevision' ) > -1 )"
            },
            "isInsertRowAfterAddParameter":{
                "expression": "!conditions.isInVRContext && ctx.panelContext.reusableParamTable === undefined && ctx.panelContext.dataProvider !== undefined && ctx.panelContext.parametersTable.parametersTableViewMode.viewModeContext !== 'showChildLines'"
            }
        },
        "onEvent": [
            {
                "eventId": "awPanel.reveal",
                "action": "getMeasurableAttrParentObjectTypes"
            },
            {
                "eventId": "getMeasurableAttrParentObjectTypes",
                "action": "getMeasurableAttrParentObjectTypes"
            },
            {
                "eventId": "lstParameterDefinitions.lovValueChanged",
                "action": "XRTLoadBatchJob"
            },
            {
                "eventId": "paramDefinitionListProvider.selectionChangeEvent",
                "action": "resetDefinitionListBox",
                "condition": "data.lstParameterDefinitions.dbValue === ''"
            },
            {
                "eventId": "Application.lovValueChanged",
                "action": "BatchJobLovChange"
            },
            {
                "eventId": "Dictionary.lovValueChanged",
                "action": "BatchJobLovChange"
            },
            {
                "eventId": "createOrUpdateMeasurableAttributeSoaCall",
                "action": "createOrUpdateMeasurableAttributeSoaCall"
            },
            {
                "eventId": "insertRowForNewParameter",
                "action": "getParameterVMOObjectsSoaCall"
            },
            {
                "eventId": "postAddParameterWithReload",
                "action": "postAddParameterBatchJob"
            },
            {
                "eventId": "refreshEvent",
                "action": "refreshAction"
            },
            {
                "eventId": "closeAddParameterPanel",
                "action": "closeAddParameterPanel"
            },
            {
                "eventId": "paramProject.paramCreated",
                "cacheEventData": true,
                "action": "refreshParamProject"
            },
            {
                "eventId": "Att1CreateParameter.prepareAttributeInput",
                "action": "prepareAttributeInputBatchJob"
            },
            {
                "eventId": "Att1CreateParameter.createParameterNew",
                "action": "createAttributeInput"
            },
            {
                "eventId": "createParametersSoaCall",
                "action": "createParametersSoaCall"
            },
            {
                "eventId": "Datatype.lovValueChanged",
                "action": "loadXrtType"
            },
            {
                "eventId": "confirmWriteAccessError",
                "message": "createParameterFailMessage"
            },
            {
                "eventId": "notWriteAccessError",
                "message": "paramCreateFail"
            }
        ],
        "messages":
        {
            "createParamFailureMessage":{
                "messageType": "ERROR",
                "messageText": "{{errorCode}}"
            },
            "createParameterFailMessage":{
                "messageType": "ERROR",
                "messageText": "{{i18n.createParameterFailureMessage}}",
                "messageTextParams": [
                    "{{data.errorDetails.paramName}}",
                    "{{subPanelContext.selectionData.selected[0].props.object_string.uiValues[0]}}"
                ]
            },
            "paramCreateFail":{
                "messageType": "ERROR",
                "messageText": "{{i18n.att1FailureMessage}}",
                "messageTextParams": [
                    "{{data.errorDetails.message}}"
                ]
            }
        },
        "i18n":
        {
            "attributeDefinition": [ "Att1AttrMappingMessages" ],
            "UnitOfMeasure": [ "Att1Messages" ],
            "Datatype": [ "Att1Messages" ],
            "Application": [ "Att1Messages" ],
            "add": [ "Att1Messages" ],
            "Engineering" : [ "Att1Messages" ],
            "Dictionary": [ "Att1Messages" ],
            "Description" : [ "Att1Messages" ],
            "Name" : [ "Att1Messages" ],
            "Goal" : [ "Att1Messages" ],
            "Min" : [ "Att1Messages" ],
            "Max" : [ "Att1Messages" ],
            "SelectDefinition": [ "Att1AttrMappingMessages" ],
            "createParameterFailureMessage": [ "Att1AttrMappingMessages" ],
            "att1FailureMessage": [ "Att1AttrMappingMessages" ],
            "Double" : [ "Att1Messages" ]
        },
        "lifecycleHooks": {
            "onInit": "reveal",
            "onUpdate": [
                {
                    "action": "performPostProcessingAfterXRTLoaded",
                    "observers": [
                        "viewModel.atomicData.xrtState.xrtTypeLoaded"
