        },
        "unsubscribeEvents": {
            "actionType": "JSFunction",
            "method": "unsubscribeEvents",
            "inputData": {
            },
            "deps": "js/Arm0CompareHistoryService"

        },
        "registerContext": {
            "actionType": "JSFunction",
